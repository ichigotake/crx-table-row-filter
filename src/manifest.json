{
    "name":"table-rows-filter",
    "version":"0.1.0",
    "manifest_version": 2,
    "description":"Chrome extension to provide the filter table rows.",
    "permissions":["contextMenus"],
    "background": {
        "scripts": ["js/background.js"]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/main.js", "js/zepto.min.js"]
    }]
}

